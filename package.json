{
  "name": "frontend-boilerplate",
  "version": "0.0.0",
  "description": "frontend boilerplate",
  "private": true,
  "scripts": {
    "start": "npm run hot",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --open --progress --hot",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --open --progress --watch",
    "build": "npm run production",
    "dev": "npm run development",
    "prod": "npm run production",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress"
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.13.17",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.10.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "cssnano": "^5.0.1",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "jquery": "^3.6.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.10",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.3",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^11.0.1",
    "sass-resources-loader": "^2.2.1",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "svgo": "^2.3.0",
    "svgo-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "4.0.0-beta.2",
    "webpack-manifest-plugin": "^3.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2"
  }
}
