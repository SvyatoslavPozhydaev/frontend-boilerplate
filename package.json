{
  "name": "frontend-boilerplate",
  "version": "0.0.0",
  "description": "frontend boilerplate",
  "private": true,
  "scripts": {
    "start": "npm run hot",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --open --progress --hot --inline",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --open --progress --watch",
    "build": "npm run production",
    "dev": "npm run development",
    "prod": "npm run production",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress"
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "cssnano": "^4.1.10",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "extract-loader": "^2.0.1",
    "file-loader": "^6.0.0",
    "html-loader": "^0.5.5",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass-loader": "^10.0.1",
    "sass-resources-loader": "^2.1.0",
    "style-loader": "^1.2.1",
    "svg-inline-loader": "^0.8.2",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6"
  }
}
